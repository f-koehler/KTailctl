{
  "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
  "C/C++ Include Guard.Macro Type": "Filepath",
  "C/C++ Include Guard.Prefix": "TAILCTL_",
  "files.associations": {
    "*.moc": "cpp",
    "qlineseries": "cpp",
    "qxyseries": "cpp"
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true,
    "**/__pycache__/": true,
    ".flatpak/**": true,
    "_build/**": true
  }
}
